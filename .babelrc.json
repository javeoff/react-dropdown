{
  "presets": [
    [
      "@babel/preset-env",
      { "useBuiltIns": "usage", "corejs": { "version": "3" }, "modules": false }
    ],
    "@babel/preset-typescript",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": [
        "@babel/plugin-transform-react-display-name"
      ]
    }
  }
}
